# RAG System Configuration Template
# Copy this to .env file in the same directory

# Admin API Key (leave empty for development mode - no auth required)
ADMIN_API_KEY=
RAG_ADMIN_API_KEY=

# API Endpoints
RAG_API_BASE=http://127.0.0.1:8000
OLLAMA_BASE_URL=http://127.0.0.1:11434

# Model Configuration (optional)
GENERATION_BACKEND=ollama
GENERATION_MODEL=llama3.2:3b
EMBEDDING_BACKEND=sentence-transformers
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Security (optional - for production)
# ADMIN_API_KEY=your_secure_admin_key_here
# RAG_ADMIN_API_KEY=your_secure_admin_key_here

# Development Mode
LOG_LEVEL=INFO
ENVIRONMENT=development
